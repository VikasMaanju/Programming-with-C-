#include <iostream>

using namespace std;

int main()
{
    int n;
    
    cout<<"Enter the number of rows you want to print"<<endl;
    cin>>n;
    cout<<endl;
    
    
    
    for(int i=n ; i>=1 ;i--){
        for(int n=i ; n>=1 ;n--){
        
            cout<<"i";
        }
        cout<<endl;
    }
    
    return 0;
}
