#include<iostream>
using namespace std;
int main(){
    int a=8;
    char c ='A';
    cout<<endl;
    for(int i =1; i<=a; i++){
        for(int k=i; k>0; k--){
            if(c>'Z'){
                c='A';
            }


            cout<<c++<<" ";
        }
        
        
        
        cout<<endl;
    }
    return 0;
}
  

    
    
