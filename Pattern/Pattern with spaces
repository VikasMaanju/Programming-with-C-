#include<iostream>
using namespace std;

int main(){
    int n=5;
    

    for(int i=n ; i>=1 ; i--){           
        //here s is taken for space.
        for(int s=i-1 ;s>=1 ;s--){
            cout<<" ";
        }
        //here k is taken for printing *
        for(int k=n-i ; k>= 0; k--){
            cout<<"*";
        }
        cout<<endl;
    }
    
    return 0;
}
