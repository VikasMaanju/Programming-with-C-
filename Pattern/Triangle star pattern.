#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter the number of line you want  to print"<<endl;
    cin>>n;
    cout<<endl;
    
    for(int i=1; i<=n;i++){
        
        for(int c=1;c<=i;c++){
            cout<<"* ";

        }
        cout<<endl;
    }
    return 0;




}

