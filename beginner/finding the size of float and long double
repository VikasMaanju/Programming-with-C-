#include<iostream>
using namespace std;
int main(){
    float a=50.5f;
    long double b=50.5l;
    
    cout<<"The size of 50.5 is  "<<sizeof(50.5)<<endl;
    cout<<"The size of 50.5l is  "<<sizeof(50.5l)<<endl;
    cout<<"The size of 50.5L is  "<<sizeof(50.5L)<<endl;
    cout<<"The size of 50.5f is  "<<sizeof(50.5f)<<endl;
    cout<<"The size of 50.5F is  "<<sizeof(50.5F)<<endl;
    return 0;
}
