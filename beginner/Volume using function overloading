#include<iostream>
using namespace std;
//Calculate the volume of cylinder
int volume(int r,int h){
    return(3.14*r*r*h);
}
//Calculate the volume of cuboid
int volume(int l, int b, int h){
    return(l*b*h);
}
//Calculate the volume of cube
int volume(int a){
    return(a*a*a);
}

int main(){
    cout<<"Volume of cylinder of 4 and is "<< volume(4,6) <<endl;
    cout<<"Volume of cuboid length 4 bredth 6 and height 8 is  "<< volume(4,6,8) <<endl;
    cout<<"Volume of cube having side 5 is " << volume(5) <<endl;

    
    return 0;
}
